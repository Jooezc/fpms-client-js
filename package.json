{
  "name": "fpms-client-dev",
  "version": "1.0.0",
  "description": "fpms api sdk",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangzcn/fpms-client-dev.git"
  },
  "keywords": ["fpms"],
  "author": "sinonet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liangzcn/fpms-client-dev/issues"
  },
  "homepage": "https://github.com/liangzcn/fpms-client-dev#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "lib/**/*"
  ]
}
